    <Color x:Key="Color2">#635283</Color>
    <Color x:Key="Color3">#4CA1A3</Color>
    <Color x:Key="Color4">#7fd48a</Color>
    <Color x:Key="Color5">Black</Color>
    <Color x:Key="Color6">#46c156</Color>
